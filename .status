# Skip non-test files ending with "_test".
packages/*: Skip
*/packages/*: Skip
*/*/packages/*: Skip
*/*/*/packages/*: Skip
*/*/*/*packages/*: Skip
*/*/*/*/*packages/*: Skip
lib/*: Skip

# Only run tests from the build directory, since we don't care about the
# difference between transformed an untransformed code.
test/*: Skip

build/test/scheduled_server_test: Pass, Fail # 13524
build/test/scheduled_process_test: Pass, Slow # Issue 9231
build/test/scheduled_stream/stream_matcher_test: Pass, Slow

[ $runtime == vm && $mode == debug]
build/test/scheduled_server_test: Pass, Fail, Slow # 13524

[ $runtime == ie10 || $runtime == ie11 ]
build/test/scheduled_test/current_schedule_errors_test: Skip # Issue 21007
build/test/scheduled_test/nested_task_test: Timeout # Skip 21007
build/test/scheduled_test/out_of_band_task_test: Skip # Issue 21007
build/test/scheduled_test/signal_error_test: Skip # Issue 21007
build/test/scheduled_test/task_return_value_test: Skip # Issue 21007
build/test/scheduled_test/timeout_test: Skip # Issue 21007
build/test/scheduled_test/wrap_async_test: Skip # Issue 21007
build/test/scheduled_test/wrap_future_test: Skip # Issue 21007

[ $compiler == none && ( $runtime == dartium || $runtime == drt || $runtime == ContentShellOnAndroid) ]
build/test/descriptor/*: RuntimeError # 13921
build/test/scheduled_future_matchers_test: RuntimeError # 13921
build/test/scheduled_process_test: RuntimeError # 13921
build/test/scheduled_stream/stream_matcher_test: RuntimeError # 13921
build/test/scheduled_test/*: RuntimeError # 13921

[ $browser ]
build/test/scheduled_process_test: Skip # Uses dart:io
build/test/scheduled_server_test: Skip # Uses dart:io
build/test/descriptor/*: Skip # Uses dart:io
